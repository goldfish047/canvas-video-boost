{
  "manifest_version": 3,
  "name": "Hold to Speed Up Video",
  "version": "1.0",
  "description": "Speeds up Canvas video playback when holding left-click.",
  "permissions": ["scripting", "storage"],
  "host_permissions": ["https://stream.sfu.ca/*"],
  
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "default_title": "Hold to Speed Up Video",
          "default_popup": "popup.html"

  },
  "icons": {
  "16": "icon.png",
  "48": "icon.png",
  "128": "icon.png"
  },


  "content_scripts": [
    {
      "matches": ["https://stream.sfu.ca/*"],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ]
}
